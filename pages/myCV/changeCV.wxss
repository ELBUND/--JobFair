page{
  background-color: #e4e4e4;
  font-size: 32rpx;
}

.addPhoto{
  height: 160rpx;
  line-height: 160rpx;
  padding: 0 20rpx;
  overflow: hidden;
  background-color: #fff;
  border-top: 1rpx solid #e4e4e4;
}

.title{
  float: left;
}

.photo{
  float: right;
  height: 100%;
  display: flex;
  align-items: center;
  margin-right: 20rpx;
}

.photo image{
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}

.right{
  float: right;
  height: 100%;
  display: flex;
  align-items: center;
}

.right image{
  width: 40rpx;
  height: 40rpx;
}

.head{
  padding-left: 20rpx;
  margin: 10rpx 0;
}

.main{
  padding-bottom: 80rpx;
}

.row{
  height: 120rpx;
  line-height: 120rpx;
  padding: 0 20rpx;
  overflow: hidden;
  border-bottom: 1rpx solid #e4e4e4;
  background-color: #fff;
}

.t_row{
  height: 400rpx;
  padding: 0 20rpx;
  background-color: #fff;
}

.content{
  float: left;
  height: 100%;
  display: flex;
  align-items: center;
  margin-left: 40rpx;
  z-index: 0;
}

.content input{
  z-index: 0;
  width: 480rpx;
}

.pickerStyle{
  color: #4a7deb;
}

.content image{
  width: 40rpx;
  height: 40rpx;
}

.project{
  margin: 20rpx;
  background: #fff;
}

textarea{
  width: 650rpx;
  height: 380rpx;
  z-index: 0;
}

.del{
  float: right;
  height: 100%;
  display: flex;
  align-items: center;
  margin-left: 20rpx;
}

.del image{
  width: 50rpx;
  height: 50rpx;
}

.city_select_wrap{
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, .4);
  overflow: hidden;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 999;
}

.city_select_body{
  width: 100%;
  height: 100%;
  overflow: hidden;
  background-color: #fff;
  border-radius: 4px;
}

.c_title{
  padding: 0 30rpx;
  height: 100rpx;
  font-size: 34rpx;
}

.c_main{
  height: 1020rpx;
  border-bottom: 1rpx solid #e4e4e4;
}

.c_confirm{
  width: 100%;
  height: 100rpx;
  padding: 0 20rpx;
  display: flex;
  align-items: center;
}

.c_confirm .showSelected{
  font-size: 34rpx;
  color: #4a7deb;
  width: 460rpx;
}

.summary{
  margin: 20rpx;
  padding: 20rpx;
  background-color: #fff;
}

.summary_item{
  display: flex;
  align-items: center;
}

.summary_item image{
  width: 40rpx;
  height: 40rpx;
}

.summary_item input{
  width: 560rpx;
}

.btnSave{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
}

.btnSave{
  z-index: 0;
}